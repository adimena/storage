{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["jest", "prettier"],
    "rules": {
      "class-methods-use-this": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    },
    "env": {
      "node": true,
      "jest/globals": true
    }
  }
  